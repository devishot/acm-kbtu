<h1>Upload Problems for <%= @contest.name%> </h1>

<%= form_tag({:action => :problems_unzip, :method => "post"}, :multipart => true) do %>
  <%= hidden_field_tag :contest_id, @contest.path %>

  <%= file_field_tag 'archive' %>
  <%= submit_tag("Upload") %>
<% end %>

<%= link_to 'Back', contests_path %>
